{
  "github": {
    "enabled": false
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "X-Robot-Tag",
          "value" : "noindex"
        },
        {
          "key" : "Cache-Control",
          "value" : "s-maxage=60, stale-while-revalidate"
        }
      ]
    }
  ]
}

